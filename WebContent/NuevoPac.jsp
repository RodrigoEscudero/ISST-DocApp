<form action="NuevoPacServlet">



	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="nombre"> Nombre</label> <input type="text" name="nombre"
					placeholder="Nombre" />
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="apellido1"> Primer Apellido</label> <input type="text"
					name="apellido1" placeholder="Primer Apellido" />
			</div>
		</div>
		<div class="col-md-4"></div>
		<div class="col-md-2">
			<div class="form-group">
				<label for="apellido2"> Segundo Apellido</label> <input type="text"
					name="apellido2" placeholder="Segundo Apellido" />
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="nacimiento"> Fecha de Nacimiento </label> <input
					type="date" name="nacimiento" placeholder="Formato DD/MM/YYYY" />
			</div>
		</div>
		<div class="col-md-4"></div>
		<div class="col-md-2">
			<div class="form-group">
				<label for="dni"> Nombre</label> <input type="text" name="dni"
					placeholder="D.N.I." />
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="domicilio"> Domicilio</label> <input type="text"
					name="domicilio" placeholder="Domicilio" />
			</div>
		</div>

	</div>


	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="telefono1"> Teléfono 1</label> <input type="text"
					name="telefono1" placeholder="Teléfono 1" />
			</div>
		</div>
		<div class="col-md-4"></div>
		<div class="col-md-2">
			<div class="form-group">
				<label for="telefono2"> Teléfono 2</label> <input type="text"
					name="telefono2" placeholder="Teléfono 2" />
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-md-2">
			<div class="form-group">
				<label for="email"> Email</label> <input type="text" name="email"
					placeholder="Email" />
			</div>
		</div>
		<div class="col-md-4"></div>
		<div class="col-md-2">
			<div class="form-group">
				<label for="password"> Contraseña</label> <input type="password"
					name="password" placeholder="Contraseña" />
			</div>
		</div>
	</div>

	<button class="btn btn-primary"type="submit">Registrarse</button>
</form>
